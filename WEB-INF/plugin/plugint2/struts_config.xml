<?xml version="1.0" encoding="shift_jis" ?>

<!DOCTYPE struts-config PUBLIC
"-//Apache Software Foundation//DTD Struts Configuration 1.2//EN"
"http://jakarta.apache.org/struts/dtds/struts-config_1_2.dtd">

<struts-config>

<!-- ========== Form Bean Definitions =============== -->
<form-beans>

<!-- typeには作成したFormクラスをパッケージ名から記述します -->
<form-bean
name="helloworldFormt2"
type="jp.groupsession.v2.plugint2.helloworldt2.HelloWorldFormt2" />

</form-beans>

<!-- ========== Action Mapping Definitions ============= -->
<action-mappings>

<!-- HelloWorld -->
<!-- nameにはform-beanに記述したnameと同じ値を記述します -->
<!-- inputには作成したJSPファイルへのパスを記述します -->
<!-- typeには作成したActionクラスをパッケージ名から記述します -->
<action
name="helloworldFormt2"
path="/plugint2/helloworldt2"
scope="request"
input="/WEB-INF/plugin/plugint2/jsp/helloworldt2.jsp"
validate="false"
type="jp.groupsession.v2.plugint2.helloworldt2.HelloWorldActiont2">
</action>

</action-mappings>

</struts-config>